<%- include('partials/header') %>
  
  <div class="container py-md-5 container--narrow mt-4 text-center" id="blur">
	<%- include('partials/flash') %>
	<%- include('partials/profileShared') %>
  
    <div class="list-group">
			<% posts.forEach(function(post) {  %>
				<a href="/post/<%= post._id %>" class="list-group-item list-group-item-action asd">
					<div class="d-flex">
						<img class="avatar-tiny mr-3" src="<%= post.author.avatar %>">
						<strong><%= post.title %></strong>
						<div class="ml-auto"><%= post.createdDate.getDate() %>/<%= post.createdDate.getMonth() + 1 %>/<%= post.createdDate.getFullYear() %></div>
					</div>
				</a>
			<% }) %>
		</div>
		
  </div>
  
  <%- include('partials/footer') %>